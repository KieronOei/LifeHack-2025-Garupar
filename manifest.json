{
    "manifest_version": 3,
    "name": "GaruPar",
    "version": "1.0",

    "permissions": [
        "activeTab",
        "storage"
    ],

    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/icon16.png",
            "48": "/assets/icon48.png",
            "128": "/assets/icon128.png"
        }
    }
}